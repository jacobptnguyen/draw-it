{
  "expo": {
    "name": "Draw It!",
    "slug": "draw-it",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "draw-it",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Draw It! needs camera access to let you capture and share your artwork.",
        "NSPhotoLibraryUsageDescription": "Draw It! needs access to your photos to let you share your artwork and add reference images for drawing.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.drawit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.drawit"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-image-picker",
        {
          "photosPermission": "Draw It! needs access to your photos to let you share your artwork and add reference images for drawing.",
          "cameraPermission": "Draw It! needs camera access to let you capture and share your artwork."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "40ac34de-509b-4643-8bed-4fc521770490"
      }
    }
  }
}
